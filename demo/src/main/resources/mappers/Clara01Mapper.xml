<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.clara.demo.repository.Data01Dao">

  <select id="selectData01" parameterType="com.clara.demo.dto.Data01Dto"
resultType="com.clara.demo.dto.Data01Dto">
  	SELECT KEY_COL 
  	      ,COL_1
  	      ,COL_2
    FROM TBL_DATA01
    WHERE KEY_COL = #{keyCol}
  </select>
</mapper>